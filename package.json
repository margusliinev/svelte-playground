{
    "name": "svelte-playground",
    "description": "Svelte Playground is a ready-to-use environment for quick Svelte prototyping and testing.",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/margusliinev/svelte-playground"
    },
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "start": "node --env-file=.env build",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "format": "prettier --write .",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "db:check": "drizzle-kit check",
        "db:push": "drizzle-kit push",
        "db:pull": "drizzle-kit pull"
    },
    "dependencies": {
        "drizzle-kit": "0.31.0",
        "drizzle-orm": "0.42.0",
        "mysql2": "3.14.0",
        "pino": "9.6.0",
        "pino-pretty": "13.0.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "5.2.12",
        "@sveltejs/kit": "2.20.7",
        "@sveltejs/vite-plugin-svelte": "5.0.3",
        "@types/node": "22.14.1",
        "prettier": "3.5.3",
        "prettier-plugin-svelte": "3.3.3",
        "svelte": "5.27.1",
        "svelte-check": "4.1.6",
        "typescript": "5.8.3",
        "vite": "6.3.1"
    }
}
